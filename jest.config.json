{
  "collectCoverage": true,
  "moduleFileExtensions": ["js", "mjs", "ts", "json"],
  "moduleDirectories": ["source", "node_modules"],
  "transform": {
    "^.+\\.ts$": "ts-jest",
    "^.+\\.js$": "babel-jest",
    "^.+\\.mjs$": "babel-jest"
  },
  "testTimeout": 30000,
  "testRegex": "((\\.|/*.)(spec)|(test))\\.ts?$",
  "setupFilesAfterEnv": ["./jest.setup.js"]
}
